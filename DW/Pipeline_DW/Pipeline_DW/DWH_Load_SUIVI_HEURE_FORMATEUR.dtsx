          SQLTask:SqlStatementSource="INSERT INTO snifop_dwh.dwh.DWH_SUIVI_HEURE_FORMATEUR (&#xA;    DATE_SUIVI,&#xA;    HEURE_COURS_TH,&#xA;    HEURE_COURS_PR,&#xA;    FORMATEUR_SK,&#xA;    UNITE_FORMATION_ID,&#xA;    DateCreation,&#xA;    DateMaj,&#xA;    IsActif,&#xA;    SUIVI_ID_SRC&#xA;)&#xA;SELECT &#xA;    s.DATE_SUIVI,&#xA;    s.HEURE_COURS_TH,&#xA;    s.HEURE_COURS_PR,&#xA;    f.FORMATEUR_SK,        &#xA;    s.UNITE_FORMATION_ID,&#xA;    GETDATE(),&#xA;    NULL,                 &#xA;    1,                     &#xA;    s.ID                  &#xA;FROM snifop_stg.dbo.SUIVI_HEURE_FORMATEUR s&#xA;JOIN snifop_dwh.dwh.DWH_FORMATEUR  f &#xA;    ON s.FORMATEUR_ID = f.FORMATEUR_ID_SRC AND f.IsActif = 1&#xA;WHERE NOT EXISTS (&#xA;    SELECT 1 &#xA;    FROM snifop_dwh.dwh.DWH_SUIVI_HEURE_FORMATEUR p&#xA;    WHERE p.SUIVI_ID_SRC = s.ID AND p.IsActif = 1&#xA;);" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
