          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{65B9E7CE-ACE8-4114-AA4A-D88EBF89CE7D}"
              SQLTask:SqlStatementSource="INSERT INTO [snifop_dwh].[dwh].[DWH_Enquete_Employeur] (&#xA;    [ID],&#xA;    [Nom_entreprise],&#xA;    [Secteur_activite],&#xA;    [PROVINCE_SK],&#xA;    [Date_enquete],&#xA;    [Questionnaire_JSON],&#xA;    [DateCreation],&#xA;    [DateMaj],&#xA;    [IsActif]&#xA;)&#xA;SELECT &#xA;    stg.ID,&#xA;    stg.Nom_entreprise,&#xA;    stg.Secteur_activite,&#xA;    p.PROVINCE_SK,&#xA;    stg.Date_enquete,&#xA;    stg.Questionnaire_JSON,&#xA;    GETDATE() AS DateCreation,&#xA;    NULL AS DateMaj,&#xA;    1 AS IsActif&#xA;FROM [snifop_stg].[dbo].[Enquete_Employeur] stg&#xA;&#xA;-- Jointure pour récupérer le PROVINCE_SK depuis le nom de province&#xA;INNER JOIN [snifop_dwh].[dwh].[DWH_PROVINCE] p&#xA;    ON p.NOM = stg.NOM_PROVINCE&#xA;    AND p.IsActif = 1&#xA;&#xA;-- Vérifie que l'enregistrement existe dans le DWH mais est inactif&#xA;INNER JOIN (&#xA;    SELECT [ID]&#xA;    FROM [snifop_dwh].[dwh].[DWH_Enquete_Employeur]&#xA;    GROUP BY [ID]&#xA;    HAVING SUM(CASE WHEN IsActif = 1 THEN 1 ELSE 0 END) = 0&#xA;) old&#xA;    ON stg.ID = old.[ID];&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
