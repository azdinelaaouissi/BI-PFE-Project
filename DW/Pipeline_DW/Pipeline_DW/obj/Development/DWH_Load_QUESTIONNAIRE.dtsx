            <SQLTask:SqlTaskData
              SQLTask:Connection="{65B9E7CE-ACE8-4114-AA4A-D88EBF89CE7D}"
              SQLTask:SqlStatementSource="INSERT INTO snifop_dwh.dwh.ENQUETE_LAUREAT (&#xA;    ID_ENQUETE,&#xA;    DATE_ENQUETE,&#xA;    NOM_LAUREAT,&#xA;    SEXE,&#xA;    AGE,&#xA;    PROVINCE_SK,&#xA;    DATE_ENTREE,&#xA;    DATE_SORTIE,&#xA;    QUESTIONNAIRE_JSON,&#xA;    DateCreation,&#xA;    DateMaj,&#xA;    IsActif&#xA;)&#xA;SELECT &#xA;    stg.ID_ENQUETE,&#xA;    stg.DATE_ENQUETE,&#xA;    stg.NOM_LAUREAT,&#xA;    stg.SEXE,&#xA;    stg.AGE,&#xA;    p.PROVINCE_SK,&#xA;    stg.DATE_ENTREE,&#xA;    stg.DATE_SORTIE,&#xA;    stg.QUESTIONNAIRE_JSON,&#xA;    GETDATE() AS DateCreation,&#xA;    NULL AS DateMaj,&#xA;    1 AS IsActif&#xA;FROM snifop_stg.dbo.ENQUETE_LAUREAT stg&#xA;&#xA;INNER JOIN snifop_dwh.dwh.DWH_PROVINCE p&#xA;    ON p.NOM = stg.NOM_PROVINCE&#xA;    AND p.IsActif = 1&#xA;&#xA;-- Filtre pour ne prendre que les enregistrements qui existent dans la table cible&#xA;-- mais sont inactifs (IsActif = 0)&#xA;INNER JOIN (&#xA;    SELECT ID_ENQUETE&#xA;    FROM snifop_dwh.dwh.ENQUETE_LAUREAT&#xA;    GROUP BY ID_ENQUETE&#xA;    HAVING SUM(CASE WHEN IsActif = 1 THEN 1 ELSE 0 END) = 0&#xA;) old&#xA;    ON stg.ID_ENQUETE = old.ID_ENQUETE;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
