              <properties>
                <property
                  dataType="System.String"
                  description="Nom de la table à extraire."
                  expressionType="Notify"
                  name="TableName">"BITBOT"."C##SNIFOP_REFERENTIAL_INSCRIPTION"</property>
                <property
                  dataType="System.String"
                  description="Commande SQL à exécuter."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT *
FROM (
    SELECT 
        BENEFICIAIRE_ID,
        TO_CHAR(
            NVL(
                DATE_INSCRIPTION,
                TO_DATE('01-01-1970', 'DD-MM-YYYY') + 
                TRUNC(DBMS_RANDOM.VALUE(0, TO_DATE('01-01-2024', 'DD-MM-YYYY') - TO_DATE('01-01-1970', 'DD-MM-YYYY')))
            ),
            'dd/mm/YY'
        ) AS DATE_INSCRIPTION ,
        ROW_NUMBER() OVER (
            PARTITION BY BENEFICIAIRE_ID 
            ORDER BY DATE_INSCRIPTION 
        ) AS rn
    FROM C##SNIFOP_REFERENTIAL_INSCRIPTION
)
WHERE rn = 1</property>
