         */
    }

    /// <summary>
    /// This method is called once for every row that passes through the component from Entrée0.
    ///
    /// Example of reading a value from a column in the the row:
    ///  string zipCode = Row.ZipCode
    ///
    /// Example of writing a value to a column in the row:
    ///  Row.ZipCode = zipCode
    /// </summary>
    /// <param name="Row">The row that is currently passing through the component</param>


    public override void Entrée0_ProcessInputRow(Entrée0Buffer Row)
    {
        try
        {
            DateTime currentDate = DateTime.Today;

            if (!Row.DATENAISSANCE_IsNull && !string.IsNullOrWhiteSpace(Row.DATENAISSANCE))
            {
                DateTime birthDate = ConvertToFullDate(Row.DATENAISSANCE);
                int age = currentDate.Year - birthDate.Year;
                if (birthDate > currentDate.AddYears(-age)) age--;
                Row.age = age;
            }
            else if (!Row.DATEINSCRIPTION_IsNull && !string.IsNullOrWhiteSpace(Row.DATEINSCRIPTION))
            {
                DateTime inscriptionDate = ConvertToFullDate(Row.DATEINSCRIPTION);
                int age = Math.Abs(currentDate.Year - inscriptionDate.Year);

                if (inscriptionDate > currentDate.AddYears(-age)) age--;
                Row.age = age + 18; // ajout des 18 ans par défaut
            }
            else
            {
                Row.age = 0;
            }
        }
        catch
        {
            Row.age = 0; // en cas d'erreur de parsing
        }
    }

    // Fonction pour convertir une date "dd/MM/yy" en "dd/MM/yyyy" avec logique des années personnalisée
    private DateTime ConvertToFullDate(string shortDate)
    {
        string[] parts = shortDate.Split('/');
        if (parts.Length != 3) throw new FormatException("Date invalide");

        int day = int.Parse(parts[0]);
        int month = int.Parse(parts[1]);
        int yearPart = int.Parse(parts[2]);

        int fullYear = (yearPart >= 0 && yearPart <= 25) ? 2000 + yearPart : 1900 + yearPart;

        return new DateTime(fullYear, month, day);
    }

